{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "declaration": false,
    "target": "ES2020",
    "moduleResolution": "node",
    "module": "commonjs",
    "noImplicitAny": false,
    "skipLibCheck": true,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "preserveSymlinks": true
  }
}
