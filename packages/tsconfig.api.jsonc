{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "declaration": false,
    "target": "es6",
    "moduleResolution": "node",
    "module": "commonjs",
    "lib": ["es2015", "es2017", "es2019", "es6", "es7", "esnext.asynciterable"],
    "noImplicitAny": false,
    "skipLibCheck": true,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "preserveSymlinks": true
  }
}
